#include <iostream>
#include <math.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */
using namespace std;

int main(int argc, char** argv) {
	double a,c,d,x,y;
	cout<<"Vvedit C="; cin>>c;
	cout<<"Vvedit D="; cin>>d;
	cout<<"Vvedit X="; cin>>x;
	cout<<"Vvedit Y="; cin>>y;
	if (x<y) 
	{
	a=exp(x*y)+d*log(x);}
	else
	{
	a=pow(x+c,1.0/3.0)+log(y);}
	cout<<"A="<<a<<endl;

	return 0;
}
