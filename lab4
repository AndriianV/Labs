#include <iostream>
#include <math.h>
/* run this program using the console pauser or add your own getch, system("pause") or input loop */
using namespace std;
int main(int argc, char** argv)
 {
 	double a,i,s=0;
 	cout<<"A="; cin>>a;
 	for (i=1;i<=10;i++)
 	s+=pow((a*i+2),2);
 	cout<<"S="<<s<<endl;
	return 0;
}
